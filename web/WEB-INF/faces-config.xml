<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2" xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee 
    http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">

    <managed-bean>
        <managed-bean-name>diagramView</managed-bean-name>
        <managed-bean-class>it.cnr.isti.labsedc.bpmnpathextractorgui.DiagramView</managed-bean-class>
        <managed-bean-scope>view</managed-bean-scope>
        <managed-property>
            <property-name>fileUploadView</property-name>
            <property-class>it.cnr.isti.labsedc.bpmnpathextractorgui.FileUploadView</property-class>
            <value>#{fileUploadView}</value>
        </managed-property>
        <managed-property>
            <property-name>checkBoxView</property-name>
            <property-class>it.cnr.isti.labsedc.bpmnpathextractorgui.CheckBoxView</property-class>
            <value>#{checkBoxView}</value>
        </managed-property>
    </managed-bean>

    <managed-bean>
        <managed-bean-name>fileUploadView</managed-bean-name>
        <managed-bean-class>it.cnr.isti.labsedc.bpmnpathextractorgui.FileUploadView</managed-bean-class>
        <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>

    <managed-bean>
        <managed-bean-name>checkBoxView</managed-bean-name>
        <managed-bean-class>it.cnr.isti.labsedc.bpmnpathextractorgui.CheckBoxView</managed-bean-class>
        <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>

</faces-config>